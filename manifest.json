{
    "name": "Tiny Tiny RSS",
    "id": "ttrss",
    "packaging_format": 1,
    "description": {
        "en": "News feed (RSS/Atom) reader and aggregator",
        "fr": "Lecteur de flux d’actualité utilisant les protocoles RSS et Atom"
    },
    "version": "20230227~ynh1",
    "url": "http://tt-rss.org",
    "upstream": {
        "license": "GPL-3.0-only",
        "website": "https://tt-rss.org",
        "demo": "https://srv.tt-rss.org/tt-rss/",
        "admindoc": "https://tt-rss.org/wiki.php",
        "code": "https://git-gitea.tt-rss.org/fox/tt-rss"
    },
    "license": "GPL-3.0-only",
    "maintainer": {
        "name": "",
        "email": ""
    },
    "requirements": {
        "yunohost": ">= 11.1.11"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "php8.0-fpm",
        "mysql"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/ttrss",
                "default": "/ttrss"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
