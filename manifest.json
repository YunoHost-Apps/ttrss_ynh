{
  "name": "Tiny Tiny RSS",
  "id": "ttrss",
  "packaging_format": 1,
  "description": {
    "en": "News feed (RSS/Atom) reader and aggregator.",
    "fr": "Lecteur de flux d’actualité utilisant les protocoles RSS et Atom."
  },
  "version": "20200916~ynh2",
  "url": "http://tt-rss.org",
  "license": "GPL-3.0-only",
  "maintainer": {
    "name": "",
    "email": ""
  },
  "requirements": {
    "yunohost": ">= 4.1.7"
  },
  "multi_instance": true,
  "services": [
      "nginx",
      "php7.3-fpm",
      "mysql"
  ],
  "arguments": {
    "install" : [
    {
      "name": "domain",
      "type": "domain",
      "example": "domain.org"
    },
    {
      "name": "path",
      "type": "path",
      "example": "/ttrss",
      "default": "/ttrss"
      }
    ]
  }
}
